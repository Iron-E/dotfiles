{
	"callout": {
		"body": [
			"> [!${1:type}]${2:}",
			">",
			"> ${3:body}",
			"$0"
		],
		"prefix": "callout"
	},
	"checkbox": {
		"body": "- [ ] $0",
		"prefix": "checkbox"
	},
	"details": {
		"body": [
			"<details><summary>${1:Click to expand}</summary>",
			"",
			"${2:...}",
			"",
			"</details>$0"
		],
		"prefix": "details"
	},
	"mermaid": {
		"body": [
			"```mermaid",
			"${1:flowchart TB}",
			"\t${2:}",
			"```$0"
		],
		"prefix": "mermaid"
	},
	"obsidian-callout": {
		"body": [
			"> [!${1|info,todo,tip,hint,important,success,check,done,question,help,faq,warning,caution,attention,failure,fail,missing,danger,error,bug,example,quote,cite|}]${2:}",
			">",
			"> ${3:body}",
			"$0"
		],
		"prefix": "obsidian-callout"
	},
	"pull-request": {
		"body": [
			"# Summary",
			"",
			"${1:description}",
			"",
			"## Checklist",
			"",
			"- [ ] ${2:}",
			"",
			"## Items for Discussion",
			"",
			"- [ ] ${3:Is it foo}?",
			"",
			"## Related",
			"",
			"- #${4:1}",
			"",
			"## Future Work",
			"",
			"- ${5:n/a}"
		],
		"prefix": "pull-request"
	},
	"tag": {
		"body": "[[${1:}|${2:$TM_SELECTED_TEXT}]]$0",
		"prefix": "tag"
	}
}
